{
    "root": "apps/discover-coffee-shops-e2e",
    "sourceRoot": "apps/discover-coffee-shops-e2e/src",
    "projectType": "application",
    "targets": {
        "e2e": {
            "executor": "@nrwl/cypress:cypress",
            "options": {
                "cypressConfig": "apps/discover-coffee-shops-e2e/cypress.json",
                "devServerTarget": "discover-coffee-shops:serve"
            },
            "configurations": {
                "production": {
                    "devServerTarget": "discover-coffee-shops:serve:production"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "apps/discover-coffee-shops-e2e/**/*.{js,ts}"
                ]
            }
        }
    },
    "tags": [],
    "implicitDependencies": ["discover-coffee-shops"]
}
